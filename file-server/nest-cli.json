{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "file": {
      "type": "library",
      "root": "libs/file",
      "entryFile": "index",
      "sourceRoot": "libs/file/src",
      "compilerOptions": {
        "tsConfigPath": "libs/file/tsconfig.lib.json"
      }
    },
    "hash": {
      "type": "library",
      "root": "libs/hash",
      "entryFile": "index",
      "sourceRoot": "libs/hash/src",
      "compilerOptions": {
        "tsConfigPath": "libs/hash/tsconfig.lib.json"
      }
    },
    "prisma": {
      "type": "library",
      "root": "libs/prisma",
      "entryFile": "index",
      "sourceRoot": "libs/prisma/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prisma/tsconfig.lib.json"
      }
    },
    "redis-cache": {
      "type": "library",
      "root": "libs/redis-cache",
      "entryFile": "index",
      "sourceRoot": "libs/redis-cache/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis-cache/tsconfig.lib.json"
      }
    }
  }
}